{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "windows"],
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/no-unused-state": "warn"
  }
}
